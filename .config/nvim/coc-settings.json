{
  "diagnostic.displayByAle": true,
  "svelte.enable-ts-plugin": true,
  "javacript": {
    "inlayHints": {
      "parameterNames": {
        "enabled": "all"
      },
      "parameterTypes": {
        "enabled": true
      },
      "variableTypes": {
        "enabled": true
      },
      "propertyDeclarationTypes": {
        "enabled": true
      },
      "functionLikeReturnTypes": {
        "enabled": true
      },
      "enumMemberValues": {
        "enabled": true
      }
    }
  },
  "typescript": {
    "inlayHints": {
      "parameterNames": {
        "enabled": "all"
      },
      "parameterTypes": {
        "enabled": true
      },
      "variableTypes": {
        "enabled": true
      },
      "propertyDeclarationTypes": {
        "enabled": true
      },
      "functionLikeReturnTypes": {
        "enabled": true
      },
      "enumMemberValues": {
        "enabled": true
      }
    }
  }
}
